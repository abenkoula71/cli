{
	"name": "natanfelles/cli",
	"type": "library",
	"description": "The Framework CLI Library",
	"keywords": [
		"cli"
	],
	"homepage": "https://the-framework.gitlab.io/libraries/cli.html",
	"license": "MIT",
	"authors": [
		{
			"name": "Natan Felles",
			"email": "natanfelles@gmail.com"
		}
	],
	"require": {
		"php": ">=7.4",
		"natanfelles/language": "^1.0"
	},
	"require-dev": {
		"ext-xdebug": "*",
		"ergebnis/composer-normalize": "^2.0",
		"natanfelles/coding-standard": "^1.0",
		"phpmd/phpmd": "^2.9",
		"phpstan/phpstan": "^0.11",
		"phpunit/phpunit": "^9.3"
	},
	"autoload": {
		"psr-4": {
			"Framework\\CLI\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\CLI\\": "tests/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"scripts": {
		"post-update-cmd": [
			"composer dump-autoload"
		]
	},
	"support": {
		"email": "natanfelles@gmail.com",
		"issues": "https://gitlab.com/the-framework/libraries/cli/issues",
		"wiki": "https://gitlab.com/the-framework/libraries/cli/wikis/home",
		"source": "https://gitlab.com/the-framework/libraries/cli",
		"docs": "https://the-framework.gitlab.io/libraries/cli/docs/"
	}
}
